{
  "expo": {
    "name": "Monae",
    "slug": "monae",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "monae",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/images/ios-icon-dark.png",
        "light": "./assets/images/ios-icon-light.png",
        "tinted": "./assets/images/ios-icon-tinted.png"
      },
      "bundleIdentifier": "com.geodevcode.monae",
      "infoPlist": {
         "NSCameraUsageDescription": "Monae requires camera access for verification.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": ["android.permission.CAMERA", "android.permission.INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png",
        "backgroundColor": "#EDF2FF"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.geodevcode.monae"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#EDF2FF",
          "dark": {
            "image": "./assets/images/splash-icon.png",
            "backgroundColor": "#070933"
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.621054984778-j2ashj7ghg9em8hq28l6bev1f9np6vca"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d0e952e4-3e5e-4107-8fe4-23a4ae074f3f"
      }
    }
  }
}
